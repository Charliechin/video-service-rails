<h1>Requests for Video Support</h1>
<table class="table table-hover">
  <tr>
    <th>ID</th>
    <th>Page</th>
    <th>Date Created</th>
    <th></th>
  </tr>
  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.id %></td>
        <td><%= ticket.product_id %></td>
        <td><%= ticket.created_at %></td>
        <td><a class="btn-block btn btn-success" data-endpoint="<%= ticket.endpoint %>" href="#" >See Details</a></td>
      </tr>
    <% end %>
  </tbody>
</table>
